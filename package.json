{
  "name": "aventura",
  "displayName": "Aventura",
  "description": "A custom theme for Visual Studio Code on macOS",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.73.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Aventura Sunup",
        "uiTheme": "hc-light",
        "path": "./themes/aventura-sunup.json"
      },
      {
        "label": "Aventura Sundown",
        "uiTheme": "vs-dark",
        "path": "./themes/aventura-sundown.json"
      },
      {
        "label": "Aventura Borealis",
        "uiTheme": "vs-dark",
        "path": "./themes/aventura-borealis.json"
      },
      {
        "label": "Aventura",
        "uiTheme": "vs-dark",
        "path": "./themes/aventura.json"
      }
    ],
    "iconThemes": [
      {
        "id": "finale-icons",
        "label": "Finale Icons",
        "path": "./icons/aventura-icons.json"
      }
    ]
  },
  "prettier": {
    "trailingComma": "none",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": true,
    "printWidth": 80,
    "arrowParens": "always"
  },
  "devDependencies": {
    "postcss": "^8.4.23"
  }
}
