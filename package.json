{
  "name": "finale",
  "displayName": "Finale",
  "description": "A custom theme for Visual Studio Code on macOS",
  "version": "1.0.0",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Themes"
  ],
  "contributes": {
    "themes": [
      {
        "label": "Finale Sunrise",
        "uiTheme": "hc-light",
        "path": "./themes/finale-sunrise.json"
      },
      {
        "label": "Finale Sundown",
        "uiTheme": "vs-dark",
        "path": "./themes/finale-sundown.json"
      }
    ],
    "iconThemes": [
			{
				"id": "finale-icons",
				"label": "Finale Icons",
				"path": "./icons/finale-icons.json"
			}
		]
  }
}