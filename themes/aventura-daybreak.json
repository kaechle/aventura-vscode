"tokenColors": [
{
"name": "Comment",
"scope": [
"comment",
"punctuation.definition.comment"
],
"settings": {
"foreground": "#dddddd40",
"fontStyle": "italic"
}
},
{
"name": "String",
"scope": [
"string"
],
"settings": {
"foreground": "#e1c39d",

}
},
{
"name": "Operators",
"scope": [
"storage.type.function.arrow"
],
"settings": {
"foreground": "#a4d0bf"
}
},
{
"name": "Keyword",
"scope": [
"storage.type",
"storage.modifier",
"keyword",
"variable.language.this",
"variable.language.super",
"keyword.other.typedef",
],
"settings": {
"foreground": "#dd8c77",
"fontStyle": "italic"
}
},
{
"name": "ClassDeclName",
"scope": [
"entity.name.type.class"
],
"settings": {
"foreground": "#8cbcc4"
}
},
{
"name": "InstanceProperties",
"scope": [
"variable.other.property",
"variable.other.object.property"
],
"settings": {
"foreground": "#9dbee9"
}
},
{
"name": "OtherDeclarations",
"scope": [
"entity.name.function",
"entity.name.function.method"
],
"settings": {
"foreground": "#859ab2"
}
},
{
"name": "ModuleNames",
"scope": [
"entity.name.type.module",
"entity.other.inherited-class",
"entity.name.class"
],
"settings": {
"foreground": "#dddddd99"
}
},
{
"name": "Markup bold",
"scope": [
"markup.bold",
"markdown.bold",
],
"settings": {
"fontStyle": "bold",
"foreground": "#ddddddbb"
}
},
{
"name": "Markup heading",
"scope":  [
"markup.heading",
"markdown.heading",
],
"settings": {
"fontStyle": "",
"foreground": "#ddddddbb"
}
},
{
"name": "Markup italic",
"scope":  [
"markup.italic",
"markdown.italic",
],
"settings": {
"fontStyle": "italic",
"foreground": "#ddddddbb"
}
},
{
"name": "Markup quote",
"scope":  [
"markup.quote",
"markdown.quote",
],
"settings": {
"fontStyle": "italic",
"foreground": "#dddddd99"
}
},
{
"name": "TypeAnnotation",
"scope": [
"meta.type.annotation"
],
"settings": {
"foreground": "#92623e"
}
},
{
"name": "tag",
"scope": [
"entity.name.tag",
],
"settings": {
"foreground": "#6b91a9"

}
},
{
"name": "Punctuation",
"scope": [
"punctuation"
],
"settings": {
"foreground": "#dddddd66"
}
},
{
"name": "Attribute",
"scope": [
"meta.attribute",
"entity.other.attribute-name"
],
"settings": {
"foreground": "#add7ff"
}
},
{
"name": "Number",
"scope": [
"constant.numeric"
],
"settings": {
"foreground": "#c9808f"
}
},
{
"name": "Property",
"scope": [
"variable.other.property"
],
"settings": {
"foreground": "#98d2bd"
}
},
{
"name": "Operator",
"scope": [
"keyword.operator"
],
"settings": {
"foreground": "#a4d0bf"
}
},
{
"name": "Parameter",
"scope": [
"variable.parameter"
],
"settings": {
"foreground": "#60929d"
}
},
{
"name": "ClassType",
"scope": [
"support.type"
],
"settings": {
"foreground": "#60929d"
}
},
{
"name": "Assignment",
"scope": [
"keyword.operator.assignment"
],
"settings": {
"foreground": "#a4d0bf"
}
},
{
"name": "GQLFragmentName",
"scope": [
"entity.name.fragment.graphql",
"variable.fragment.graphql"
],
"settings": {
"foreground": "#dddddd99"
}
},
{
"name": "QuasiElement",
"scope": [
"punctuation.quasi.element"
],
"settings": {
"foreground": "#dddddd99"
}
},
{
"name": "Variable",
"scope": [
"variable.other.readwrite",
],
"settings": {
"foreground": "#ddddddaa"
}
},
{
"name": "Brace",
"scope": [
"meta.brace"
],
"settings": {
"foreground": "#ddddddaa"
}
},
{
"name": "Arithmetic",
"scope": [
"keyword.operator.arithmetic"
],
"settings": {
"foreground": "#a4d0bf"
}
},
{
"scope": "token.info-token",
"settings": {
"foreground": "#7a8da3"
}
},
{
"scope": "token.warn-token",
"settings": {
"foreground": "#a86f42"
}
},
{
"scope": "token.error-token",
"settings": {
"foreground": "#ab534c"
}
},
{
"scope": "token.debug-token",
"settings": {
"foreground": "#79545f"
}
}
]
}
