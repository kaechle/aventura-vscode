  },
  "tokenColors": [
    {
      "name": "Invisibles",
      "scope": [
        "invisible"
      ],
      "settings": {
        "foreground": "#262626"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "punctuation.definition.string.begin.python",
        "punctuation.definition.string.end.python"
      ],
      "settings": {
        "foreground": "#585858",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Docstrings",
      "scope": [
        "string.quoted.docstring.multi"
      ],
      "settings": {
        "foreground": "#585858",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted.double",
        "string.quoted.single",
        "string.quoted.triple",
        "string.quoted.other"
      ],
      "settings": {
        "foreground": "#e4dac1"
      }
    },
    {
      "name": "Operators",
      "scope": [
        "storage.type.function.arrow"
      ],
      "settings": {
        "foreground": "#bcbbbc"
      }
    },
    {
      "name": "Constants",
      "scope": [
        "constant",
        "variable",
        "constant.other",
        "variable.other.constant.object"
      ],
      "settings": {
        "foreground": "#bcbbbc"
      }
    },
    {
      "name": "Keyword",
      "scope": [
        "storage.type",
        "storage.modifier",
        "keyword",
        "variable.language.this",
        "variable.language.super",
        "keyword.other.typedef",
        "storage.type.function.python",
        "meta.function.python"
      ],
      "settings": {
        "foreground": "#e59b6f"
      }
    },
    {
      "name": "Keyword Control",
      "scope": [
        "keyword.control",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#d48460",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function"
      ],
      "settings": {
        "foreground": "#bcbbbc"
      }
    },
    {
      "name": "Support Functions",
      "scope": [
        "support.function"
      ],
      "settings": {
        "foreground": "#eaa374"
      }
    },
    {
      "name": "Methods",
      "scope": [
        "meta.method"
      ],
      "settings": {
        "foreground": "#77aabb"
      }
    },
    {
      "name": "Parameters",
      "scope": [
        "variable.parameter",
        "variable.other.readwrite",
        "meta.function-call.arguments"
      ],
      "settings": {
        "foreground": "#9dcbda"
      }
    },
    {
      "name": "Keys",
      "scope": [
        "meta.object-literal.key",
        "meta.object.member",
        "meta.objectliteral"
      ],
      "settings": {
        "foreground": "#88aba3"
      }
    },
    {
      "name": "Code Tags",
      "scope": [
        "keyword.codetag.notation",
        "string.quoted.docstring.multi.python"
      ],
      "settings": {
        "foreground": "#585858",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Module Names",
      "scope": [
        "entity.name.type.module",
        "entity.other.inherited-class",
        "entity.name.class"
      ],
      "settings": {
        "foreground": "#bcbbbc"
      }
    },
    {
      "name": "Storage Type Functions",
      "scope": [
        "storage.type.function"
      ],
      "settings": {
        "foreground": "#9dcbda"
      }
    },
    {
      "name": "Markup",
      "scope": [
        "markup.heading",
        "markup.bold",
        "markup.italic",
        "markup.quote",
        "markup.underline",
        "markup.strike",
        "markup.list",
        "markup.inserted",
        "markup.deleted",
        "markup.changed",
        "markup.ignored",
        "markup.output",
        "markup.raw",
        "markup.markdown",
        "markup.inserted.git_gutter",
        "markup.deleted.git_gutter",
        "markup.changed.git_gutter",
        "markup.ignored.git_gutter",
        "markup.untracked.git_gutter",
        "markup.fenced_code.block.markdown",
        "markup.inline.raw.markdown"
      ],
      "settings": {
        "foreground": "#bcbbbc"
      }
    },
    {
      "name": "Markup Heading",
      "scope": [
        "markup.heading",
        "markdown.heading",
        "markup.bold",
        "markdown.bold"
      ],
      "settings": {
        "foreground": "#c5c5c5"
      }
    },
    {
      "name": "Markup Italic",
      "scope": [
        "markup.italic",
        "markdown.italic"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#bcbbbc"
      }
    },
    {
      "name": "Markup Quote",
      "scope": [
        "markup.quote",
        "markdown.quote"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#bcbbbc"
      }
    },
    {
      "name": "Type Annotation",
      "scope": [
        "meta.type.annotation"
      ],
      "settings": {
        "foreground": "#c68470"
      }
    },
    {
      "name": "Tags",
      "scope": [
        "entity.name.tag",
        "support.class.component.svelte"
      ],
      "settings": {
        "foreground": "#81b3bc"
      }
    },
    {
      "name": "Attribute",
      "scope": [
        "meta.attribute",
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#bacfec"
      }
    },
    {
      "name": "Number",
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#efacbe"
      }
    },
    {
      "name": "Boolean",
      "scope": [
        "boolean"
      ],
      "settings": {
        "foreground": "#bf887a",
        "fontStyle": "italic"
      }
    },
    {
      "name": "CSS Function",
      "scope": [
        "support.function.misc.css"
      ],
      "settings": {
        "foreground": "#efd8b4"
      }
    },
    {
      "name": "CSS Tag",
      "scope": [
        "entity.name.tag.css"
      ],
      "settings": {
        "foreground": "#9baedc"
      }
    },
    {
      "name": "CSS Property Name",
      "scope": [
        "support.type.property-name.css"
      ],
      "settings": {
        "foreground": "#bcbbbc"
      }
    },
    {
      "name": "CSS Property List",
      "scope": [
        "meta.property-list.css",
        "meta.property-value.css",
        "support.constant.property-value.css"
      ],
      "settings": {
        "foreground": "#bcbbbc"
      }
    },
    {
      "name": "Operator",
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment",
        "storage.type.function.arrow"
      ],
      "settings": {
        "foreground": "#bcbbbc"
      }
    },
    {
      "name": "Variable",
      "scope": [
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#bcbbbc"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation",
        "meta.brace",
        "keyword.other.unit.percentage.css",
        "keyword.other.unit.px.css"
      ],
      "settings": {
        "foreground": "#bcbbbc"
      }
    },
    {
      "name": "Property Name",
      "scope": [
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#88aba3"
      }
    },
    {
      "scope": "token.info-token",
      "settings": {
        "foreground": "#84a5b2"
      }
    },
    {
      "scope": "token.warn-token",
      "settings": {
        "foreground": "#efc684"
      }
    },
    {
      "scope": "token.error-token",
      "settings": {
        "foreground": "#c36e77"
      }
    },
    {
      "scope": "token.debug-token",
      "settings": {
        "foreground": "#bd77a2"
      }
    }
  ]
}