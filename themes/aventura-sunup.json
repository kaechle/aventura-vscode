    "panel.background": "#ffffff",
    "panel.border": "#e5e5e5",
		"panelTitle.activeBorder": "#00000000",
		"panelTitle.activeForeground": "#808080",
		"panelTitle.inactiveForeground": "#bdbdbd",
		"panelSectionHeader.background": "#ffffff",
		"panelSection.border": "#00000088",
		"panelSectionHeader.border": "#00000088",
		"panelSectionHeader.foreground": "#dedede",
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#c0c0c0",
				"fontStyle": "italic"
			}
		},
		{
			"name": "String",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#d38f31"
			}
		},
		{
			"name": "Keyword",
			"scope": [
				"storage.type",
				"storage.modifier",
				"keyword",
				"variable.language.this",
				"variable.language.super",
				"keyword.other.typedef",
			],
			"settings": {
				"foreground": "#ed8050",
			}
		},
		{
			"name": "OtherDeclarations",
			"scope": [
				"entity.name.function",
				"entity.name.function.method"
			],
			"settings": {
				"foreground": "#476075"
			}
		},
		{
			"name": "ModuleNames",
			"scope": [
				"entity.name.type.module",
				"entity.other.inherited-class",
				"entity.name.class"
			],
			"settings": {
				"foreground": "#1a1a1a"
			}
		},
		{
			"name": "Markup Bold",
			"scope": [
				"markup.bold",
				"markdown.bold",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#1a1a1a"
			}
		},
		{
			"name": "Markup Heading",
			"scope":  [
				"markup.heading",
				"markdown.heading",
			],
			"settings": {
				"fontStyle": "bold",
				"foreground": "#1a1a1a"
			}
		},
		{
			"name": "Markup Italic",
			"scope":  [
				"markup.italic",
				"markdown.italic",
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#1a1a1a"
			}
		},
		{
			"name": "Markup Quote",
			"scope":  [
				"markup.quote",
				"markdown.quote",
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#1a1a1a"
			}
		},
		{
			"name": "Type Annotation",
			"scope": [
				"meta.type.annotation"
			],
			"settings": {
				"foreground": "#a55d39"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
			],
			"settings": {
				"foreground": "#4c6e5f"

			}
		},
		{
			"name": "Punctuation",
			"scope": [
				"punctuation"
			],
			"settings": {
				"foreground": "#a0a0a0"
			}
		},
		{
			"name": "Attribute",
			"scope": [
				"meta.attribute",
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "#5992bf"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#b35067"
			}
		},
		{
			"name": "Brace",
			"scope": [
				"meta.brace"
			],
			"settings": {
				"foreground": "#808080"
			}
		},
		{
			"name": "Property Name",
			"scope": [
				"support.type.property-name"
			],
			"settings": {
				"foreground": "#476074"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#5991c0"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#d4a45d"
			}
		},
		{
			"scope": "token.error-token",
			"settings": {
				"foreground": "#d7625d"
			}
		},
		{
			"scope": "token.debug-token",
			"settings": {
				"foreground": "#c54494"
			}
		}
	]
}
