		"editorSuggestWidget.selectedBackground": "#6ba0f9",
		"editorSuggestWidget.selectedForeground": "#ffffff",
		"editorSuggestWidget.selectedIconForeground": "#ffffff",
		"editorSuggestWidgetStatus.foreground": "#3c3c3c",

		"minimap.background": "#ffffff",
    "minimapSlider.background": "#22222211",
		"minimapSlider.hoverBackground": "#22222222",
		"minimapSlider.activeBackground": "#22222222",

		"scrollbar.shadow": "#11111122",
