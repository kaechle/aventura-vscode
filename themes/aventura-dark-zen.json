{
  "$schema": "vscode://schemas/color-theme",
  "type": "dark",
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "enumMember": {
      "foreground": "#c1c3c8"
    },
    "variable.constant": {
      "foreground": "#c1c3c8"
    },
    "variable.defaultLibrary": {
      "foreground": "#71a5d2"
    }
  },
  "tokenColors": [
    // base
    {
      "scope": [
        "keyword.control",
        "keyword.control.import.python,keyword.control.flow.python",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#ddb0e7",
        "fontStyle": "italic"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#e3d8a1"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#b4cded"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "operator.assignment",
        "keyword.operator.assignment",
        "keyword.operator.arithmetic",
        "keyword.operator.address",
        "keyword.operator.bitwise",
        "keyword.operator.channel",
        "keyword.operator.expression.instanceof",
        "keyword.operator.new",
        "keyword.operator.ternary",
        "keyword.operator.optional",
        "keyword.operator.expression.keyof",
        "keyword.operator.arithmetic,keyword.operator.comparison,keyword.operator.decrement,keyword.operator.increment,keyword.operator.relational"
      ],
      "settings": {
        "foreground": "#71a5d2"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment, punctuation.definition.comment",
        "comment.line.double-slash",
        "comment.line.double-slash",
        "string.quoted.docstring.multi.python"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#5d5e5f"
      }
    },
    {
      "scope": [
        "punctuation.definition.heading.markdown",
        "markup.fenced_code.block.markdown",
        "punctuation.separator.parameters",
        "punctuation.section.class.begin",
        "punctuation.section.class.end",
        "punctuation.separator.dict",
        "punctuation.separator.period",
        "punctuation.separator.element",
        "punctuation.separator.dictionary.pair",
        "punctuation.section.function.begin",
        "punctuation.section.function.end",
        "punctuation.definition.string.begin",
        "punctuation.definition.string.end",
        "punctuation.separator.array",
        "punctuation.support.type.property-name",
        "punctuation.separator.dictionary.key-value.json"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#636566"
      }
    },
    {
      "scope": [
        "punctuation.definition.heading.markdown",
        "markup.fenced_code.block.markdown"
      ],
      "settings": {
        "foreground": "#737576"
      }
    },
    // markdown
    {
      "scope": ["meta.paragraph.markdown"],
      "settings": {
        "foreground": "#e8edef"
      }
    },
    {
      "scope": ["markup.heading.markdown"],
      "settings": {
        "foreground": "#e8edef"
      }
    },
    // css
    {
      "scope": [
        "punctuation.separator.key-value.css",
        "punctuation.definition.entity.css",
        "punctuation.definition.constant.css"
      ],
      "settings": {
        "foreground": "#e8edef"
      }
    },
    {
      "scope": [
        "meta.property-value.css",
        "meta.property-list.css",
        "keyword.other.unit"
