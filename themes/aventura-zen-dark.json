      "settings": {
        "foreground": "#e0b6e4",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "constant.other.color.rgb-value.hex.css",
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#ec91a3",
      },
    },
    {
      "scope": [
        "entity.name.function",
        "entity.name.function.python",
        "meta.function.python",
        "support.function.magic.python",
        "keyword.control.jinja",
        "meta.scope.jinja.tag",
        "text.html.jinja"
      ],
      "settings": {
        "foreground": "#ed8d63",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "meta.function-call",
        "keyword.other.important.css",
      ],
      "settings": {
        "foreground": "#cf9f7b",
        "fontStyle": ""
      },
    },
    {
      "scope": [
        "meta.function-call.arguments",
        "meta.function-call.arguments",
        "meta.function-call.arguments.python",
        "meta.attribute.charset.html",
        "meta.tag.metadata.meta.void.html",
        "meta.tag.metadata.meta.void.html"
      ],
      "settings": {
        "foreground": "#d5c085",
      }
    },
    {
      "scope": [
        "variable.parameter.function-call",
        "meta.function.paremeter",
        "meta.property-name.css",
        "meta.property-name.css",
        "meta.property-list.css",
        "support.type.property-name.css",
        "meta.property-name.css",
        "meta.property-list.css",
        "variable.parameter.function.language.special.cls.python",
        "variable.parameter.function.language.python",
        "meta.function.parameters.python",
        "variable.other.jinja",
        "meta.scope.jinja.variable"
      ],
      "settings": {
        "foreground": "#dee3edc0",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "storage.type.function.python",
        "meta.function.python",
        "storage.type.class"
      ],
      "settings": {
        "foreground": "#dee3edd0",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "source",
        "text.html.derivative",
        "punctuation.section.function.begin.python",
        "meta.function.python",
        "source.python",
        "entity.name.function.decorator",
        "entity.other.jinja.delimiter.tag",
        "meta.function.decorator",
        "punctuation.separator.array.json",
        "punctuation.separator.dict",
        "punctuation.section.function",
        "punctuation.separator.element",
        "punctuation.definition.arguments",
        "punctuation.definition.parameters",
        "constant.character.format.placeholder",
        "punctuation.definition.dict",
        "punctuation.definition.list",
        "punctuation.separator.arguments",
        "punctuation.definition.dictionary",
        "meta.structure.dictionary.value",
        "punctuation.separator.dictionary",
        "punctuation.definition.tag.begin.html",
        "punctuation.definition.tag.end.html"
      ],
      "settings": {
        "foreground": "#dee3edd0",
        "fontStyle": "",
      },
    },
    {
      "scope": [
        "string.quoted.docstring.multi.python",
        "text.html.jinja",
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#dee3ed90"
      }
    },
    {
      "name": "Comments",
      "scope": [
        "comment",
        "string.quoted.docstring.multi",
        "punctuation.definition.comment",
        "comment.line.double-slash",
        "comment.line.double-slash",
        "string.quoted.docstring.multi.python",
        "text.html.jinja",
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#dee3ed58"
      }
    },
