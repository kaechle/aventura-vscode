      "settings": {
        "foreground": "#79c6be",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "entity.name.tag.html",
        "meta.tag.metadata.meta.void.html",
      ],
      "settings": {
        "foreground": "#79c6be",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.html",
