      "settings": {
        "foreground": "#79c6beb0",
        "fontStyle": ""
      }
    },
    {
      "scope": [
        "string.quoted.single.python",
        "string.quoted.multi",
        "string.interpolated.python",
        "meta.fstring.python",
        "string.quoted.single",
        "string.quoted.double",
