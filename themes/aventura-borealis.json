  "semanticHighlighting": true,
  "semanticTokenColors": {},
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.quoted.docstring",
        "comment.block.documentation"
      ],
      "settings": {
        "foreground": "#666668",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "constant.numeric",
        "constant.numeric.float",
        "constant.numeric.decimal",
        "constant.other.color.rgb-value.hex.css"
      ],
      "settings": {
        "foreground": "#e8abb8"
      }
    },
    {
      "scope": ["meta.function.parameters", "constant.language.boolean"],
      "settings": {
        "foreground": "#d39778"
      }
    },
    {
      "scope": [
        "entity.name.section",
        "entity.name.tag",
        "entity.name.namespace",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#7ea0cd"
      }
    },
    {
